<h1>Comments</h1>


<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Comment</th>
        </tr>
    </thead>
    <tbody>
        <% @com.each do |c|%>
        <tr>
            <td><%=c.name %> </td>
            <td><%=c.comment%></td>
        </tr>
        
        <% end %>
    </tbody>
</table>


